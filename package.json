    "sw-toolbox": "github:GoogleChrome/sw-toolbox#fa4d75d830dab951956b0fc5269efa6f3a8b6fcd"
  },
  "devDependencies": {
    "@types/react-redux": "^4.4.40",
    "@types/redux-promise-middleware": "^0.0.6",
    "app-root-path": "^2.0.1",
    "autoprefixer": "6.7.2",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "cli-highlight": "1.1.4",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "prettier": "^1.3.1",
    "promise": "7.1.1",
    "react-dev-utils": "^0.5.2",
    "style-loader": "0.13.1",
    "ts-loader": "^1.3.1",
    "tslint": "^4.0.2",
    "tslint-loader": "^3.3.0",
    "tslint-react": "^2.0.0",
    "typescript": "^2.1.4",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "scripts": {
    "start": "node frontend/scripts/start.js",
    "build": "node frontend/scripts/build.js",
    "test": "node frontend/scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "frontend/src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/frontend/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/frontend/[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/frontend/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/frontend/config/jest/typescriptTransform.js",
      "^(?!.*\\.(css|json)$)": "<rootDir>/frontend/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  }
}
